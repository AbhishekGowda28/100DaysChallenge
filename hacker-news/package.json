{
    "name": "hacker-news",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "expect": "^26.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "test:coverage": "set CI=true && react-scripts test --coverage"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.js",
            "!src/index.js",
            "!src/serviceWorker.js",
            "!src/constants/*.js",
            "src/**/*.jsx"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 65,
                "lines": 95
            },
            "src/helper/mapTime": {
                "branches": 50,
                "lines": 50,
                "statements": 50,
                "functions": 100
            }
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}